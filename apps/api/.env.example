# MyMVP API Environment Variables
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=development
PORT=3001

# API Security
API_SECRET_KEY=your-secret-api-key-here

# Loops.so Email Service
LOOPS_API_KEY=your-loops-api-key-here
ADMIN_EMAIL=admin@mymvp.io

# Supabase Database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key-here

# CORS Configuration
CORS_ORIGIN=https://mymvp.io

# Calendar Integration (optional)
CALENDLY_URL=https://calendly.com/mymvp/discovery-call

# Development Only (remove in production)
DEBUG=true